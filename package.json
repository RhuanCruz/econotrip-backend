{
  "name": "econotrip-api",
  "version": "1.0.0",
  "main": "./src/index.ts",
  "license": "MIT",
  "bin": "./build/index.js",
  "scripts": {
    "build": "tsc -p . && tscpaths -p tsconfig.json -s ./src -o ./build",
    "build2": "pkg . --out-path exec/",
    "dev": "ts-node-dev --inspect -r tsconfig-paths/register --respawn --transpile-only --ignore-watch node_modules --no-notify src/index.ts",
    "start": "ts-node --files -r tsconfig-paths/register build/index.js",
    "script": "ts-node --files -r tsconfig-paths/register",
    "swagger": "npx tsoa spec",
    "migration": "npx prisma migrate dev --name",
    "migrate": "npx prisma migrate deploy",
    "seed": "ts-node -r tsconfig-paths/register --transpile-only src/common/infra/database/prisma/seeds/index.ts"
  },
  "pkg": {
    "scripts": [
      "./build/**/*.js"
    ],
    "assets": [
      "prisma/schema.prisma",
      "prisma/migrations/**/*",
      "node_modules/figlet/fonts/Standard.flf",
      "node_modules/swagger-ui-dist/*",
      "node_modules/axios/dist/node/axios.cjs"
    ],
    "targets": [
      "node18-linux-x64",
      "node18-macos-x64",
      "node18-win-x64"
    ]
  },
  "devDependencies": {
    "@eslint/js": "^9.3.0",
    "@types/cors": "^2.8.17",
    "@types/diacritics": "^1.3.3",
    "@types/express": "^5.0.1",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/node": "^20.12.13",
    "@types/nodemailer": "^6.4.17",
    "@types/nodemailer-express-handlebars": "^4.0.5",
    "@types/swagger-ui-express": "^4.1.6",
    "@typescript-eslint/eslint-plugin": "^6.4.0",
    "eslint": "^8.0.1",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^18.0.0",
    "eslint-config-standard-with-typescript": "^43.0.1",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-n": "^15.0.0",
    "globals": "^15.3.0",
    "pkg": "^5.8.1",
    "prisma": "^5.22.0",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "tscpaths": "^0.0.9",
    "typescript": "^5.4.5"
  },
  "dependencies": {
    "@prisma/client": "^5.17.0",
    "amadeus": "^11.0.0",
    "argon2": "^0.40.3",
    "axios": "^1.7.9",
    "class-transformer": "^0.5.1",
    "cors": "^2.8.5",
    "diacritics": "^1.3.0",
    "dotenv": "^16.4.5",
    "express": "^5.1.0",
    "express-async-errors": "^3.1.1",
    "express-handlebars": "^8.0.3",
    "firebase-admin": "^13.4.0",
    "handlebars": "^4.7.8",
    "http-status-codes": "^2.3.0",
    "inversify": "^6.0.2",
    "jsonwebtoken": "^9.0.2",
    "nodemailer": "^7.0.5",
    "nodemailer-express-handlebars": "^7.0.0",
    "openai": "^4.91.0",
    "pg": "^8.11.5",
    "reflect-metadata": "^0.2.2",
    "serpapi": "^2.1.0",
    "swagger-ui-express": "^5.0.1",
    "tsoa": "^6.2.1",
    "typeorm": "^0.3.20",
    "typeorm-extension": "^3.5.1",
    "typeorm-transactional": "^0.5.0",
    "winston": "^3.13.0",
    "winston-daily-rotate-file": "^5.0.0",
    "zod": "^3.23.8",
    "zod-to-json-schema": "^3.23.0"
  },
  "packageManager": "yarn@1.22.22+sha512.a6b2f7906b721bba3d67d4aff083df04dad64c399707841b7acf00f6b133b7ac24255f2652fa22ae3534329dc6180534e98d17432037ff6fd140556e2bb3137e"
}
